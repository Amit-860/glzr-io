{
	"$schema": "https://github.com/glzr-io/zebar/raw/v3.0.1/resources/zpack-schema.json",
	"name": "overline-zebar",
	"version": "0.0.0",
	"description": "Fully featured topbar using React+Vite, Tailwind & Framer Motion. ",
	"tags": [],
	"previewImages": ["resources/preview-image-1.png"],
	"repositoryUrl": "https://github.com/mushfikurr/overline-zebar",
	"widgets": [
		{
			"name": "overline",
			"htmlPath": "./dist/index.html",
			"zOrder": "normal",
			"shownInTaskbar": false,
			"focused": false,
			"resizable": false,
			"transparent": true,
			"includeFiles": [
				"dist/index.html",
				"dist/assets/**",
				"dist/config.json",
				"dist/geistMono/**",
				"dist/Jet*",
				"README.md"
			],
			"caching": {
				"defaultDuration": 604800,
				"rules": []
			},
			"privileges": {
				"shellCommands": []
			},
			"presets": [
				{
					"name": "default",
					"anchor": "top_left",
					"offsetX": "6px",
					"offsetY": "2px",
					"width": "99.4%",
					"height": "30px",
					"monitorSelection": {
						"type": "all"
					},
					"dockToEdge": {
						"enabled": false,
						"edge": null,
						"windowMargin": "0px"
					}
				}
			]
		}
	]
}
